<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.NhanHieu" table="nhanhieu" schema="quanlykho">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="ten">
            <column name="ten" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="trangthai">
            <column name="trangthai" sql-type="int(1)"/>
        </property>
        <property name="mota">
            <column name="mota" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>